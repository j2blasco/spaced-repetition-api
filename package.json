{
  "type": "module",
  "scripts": {
    "boundaries": "ts-boundaries",
    "build:mcp": "npm run clean:mcp && tsup --config tsup.mcp.config.ts",
    "build:server": "npm run clean:server && tsup",
    "build": "npm run build:server && npm run build:mcp",
    "clean:mcp": "shx rm -rf dist/mcp",
    "clean:server": "shx rm -rf dist/server",
    "dev:mcp-http": "tsx src/api/mcp/start-http-server.ts",
    "dev": "tsx --watch src/index.ts",
    "lint:fix": "npm run boundaries && eslint src --ext .ts --fix",
    "lint": "npm run boundaries && eslint src --ext .ts",
    "script": "tsx --tsconfig ./scripts/tsconfig.json",
    "start:mcp-http": "node dist/mcp/start-mcp-http-server.js",
    "start": "node dist/server/index.js",
    "test:watch": "vitest",
    "test": "vitest run --bail 1",
    "tsx": "tsx"
  },
  "dependencies": {
    "@j2blasco/ts-crud": "^0.1.9",
    "@j2blasco/ts-env": "^0.0.3",
    "@j2blasco/ts-pipe": "^0.1.5",
    "@j2blasco/ts-result": "^0.1.7",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@j2blasco/ts-boundaries": "^0.1.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "shx": "^0.4.0",
    "supertest": "^7.1.3",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "~5.7.2",
    "vitest": "^3.2.4"
  }
}
