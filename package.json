{
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "shx rm -rf dist types",
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "boundaries": "ts-boundaries",
    "lint:fix": "npm run boundaries && eslint src --ext .ts --fix",
    "lint": "npm run boundaries && eslint src --ext .ts",
    "script": "tsx --tsconfig ./scripts/tsconfig.json",
    "test": "vitest run --bail 1",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@j2blasco/ts-crud": "^0.1.9",
    "@j2blasco/ts-env": "^0.0.3",
    "@j2blasco/ts-pipe": "^0.1.5",
    "@j2blasco/ts-result": "^0.1.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "module-alias": "^2.2.3",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@j2blasco/ts-boundaries": "^0.1.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint": "^9.29.0",
    "shx": "^0.4.0",
    "supertest": "^7.1.3",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "~5.7.2",
    "vitest": "^3.2.4"
  }
}
